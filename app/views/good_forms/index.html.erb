<h1>Good Form - Teams</h1>

<%= form_tag goodForms_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>

<ul>

<% for team in @teams %>
	<li>
		<%= link_to team.name, goodForm_path(team) %>
	</li>
<% end %>


</ul>


