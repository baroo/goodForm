<h1>Good Form - Teams</h1>

<a name"top"></a>

<%= form_tag goodForms_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>


<tr>
	<th>Max Home Goals Scored</th>
	<th><%= @maxHg %></th>
	<th>Name</th>
	<th><%= @team1 %></th>
</tr>

<ul>

<% for team in @teams %>
	<li>
		<%= link_to team.name, goodForm_path(team.id) %>
	</li>
<% end %>

</ul>

<a href="#top">Back To Top</a>


